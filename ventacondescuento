#include <iostream>
using namespace std;

void ventasemanal(int& valor);

int main()
{
    int valor = 0;   
    cout << "ingrese el monto"<< endl;
    cin >> valor;
    ventasemanal(valor);

    cout << "El monto final con el descuento es: " << valor << endl;

    return 0;
}

void ventasemanal(int& valor) {

    valor = valor - (valor * 0.1);
}
